#!/usr/bin/env bash
font="Noto Sans Mono:style=Regular:pixelsize=18:antialias=true:autohint=true";
col_gray1='#222222'
col_gray2='#444444'
col_gray3='#bbbbbb'
col_gray4='#eeeeee'
col_cyan='#005577'
mon=0
exec dmenu_run -m $mon -fn $font -nb $col_gray1 -nf $col_gray3 -sb $col_cyan -sf $col_gray4 "$@"
